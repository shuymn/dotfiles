// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "cmd+k shift+v",
        "command": "mdmath.showPreview",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+.",
        "command": "-mdmath.showPreview",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+v",
        "command": "-workbench.view.git"
    },
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    }
]
